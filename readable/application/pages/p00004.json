 ,"Processes":[
    {
      "id":"122446938521384248"
     ,"Identification":{
        "Name":"Criar usu\u00E1rio"
       ,"Type":"Executar C\u00F3digo"
       ,"Execution Chain":"None"
      }
     ,"Source":{
        "Location":"Local Database"
       ,"Language":"PL\/SQL"
       ,"PL\/SQL Code":"DECLARE\n    v_status VARCHAR2(20);\n    v_mensagem VARCHAR2(4000);\n    v_acao VARCHAR2(20);\nBEGIN\n    v_acao := \u0027Criar usu\u00E1rio\u0027;\n    -- Registra um log da tentativa de cria\u00E7\u00E3o do usu\u00E1rio\n    LOG_INSERT(:APP_USER, v_acao, \u0027INFO\u0027, (\u0027Tentativa de cria\u00E7\u00E3o do usu\u00E1rio \u0027 || \u0027\"\u0027 || UPPER(:P4_USUARIO) || \u0027\"\u0027 || \u0027. E-mail: \u0027 || :P4_EMAIL));\n    \n    BEGIN\n        pk_gis.criar_usuario(:P4_USUARIO, :P4_SENHA);\n        \n        -- Se a cria\u00E7\u00E3o do usu\u00E1rio for bem-sucedida, define o status como \u0027SUCESSO\u0027 e registra uma mensagem de sucesso\n        v_status := \u0027SUCESSO\u0027;\n        v_mensagem := \u0027Usu\u00E1rio criado com sucesso. E-mail enviado para: \u0027 || :P4_EMAIL;\n    EXCEPTION\n        WHEN OTHERS THEN\n            -- Define o status como \u0027ERRO\u0027 e registra uma mensagem de erro com a descri\u00E7\u00E3o do erro SQL\n            v_status := \u0027ERRO\u0027;\n            v_mensagem := \u0027Erro: \u0027 || SQLERRM;\n            \n            LOG_INSERT(:APP_USER, v_acao, v_status, v_mensagem);\n            \n            RAISE;\n    END;\n\n    LOG_INSERT(:APP_USER, v_acao, v_status, v_mensagem);\nEND;\n"
      }
     ,"Execution":{
        "Sequence":10
       ,"Point":"Processing"
       ,"Run Process":"Once Per Page Visit (default)"
      }
     ,"Success Message":{
        "Success Message":"Usu\u00E1rio \u0026P4_USUARIO. criado com sucesso."
      }
     ,"Error":{
        "Error Message":"Erro: N\u00E3o foi poss\u00EDvel criar o usu\u00E1rio.\n\nDetalhes: #SQLERRM_TEXT#"
       ,"Display Location":"Inline in Notification"
      }
    }
