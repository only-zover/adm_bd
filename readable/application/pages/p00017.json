     ,"Identification":{
        "Name":"Atualizar senha"
       ,"Type":"Executar C\u00F3digo"
       ,"Execution Chain":"None"
      }
     ,"Source":{
        "Location":"Local Database"
       ,"Language":"PL\/SQL"
       ,"PL\/SQL Code":"DECLARE\n    v_status VARCHAR2(20);\n    v_mensagem VARCHAR2(4000);\n    v_acao VARCHAR2(20);\nBEGIN\n    v_acao := \u0027Alterar senha\u0027;\n    LOG_INSERT(:APP_USER, v_acao, \u0027INFO\u0027, (\u0027Tentativa de altera\u00E7\u00E3o de senha do usu\u00E1rio \u0027 || \u0027\"\u0027 || UPPER(:APP_USER) || \u0027\" pela primeira vez.\u0027));\n    \n    BEGIN\n        pk_gis.alterar_senha(:APP_USER, :P17_CONFIRMAR_SENHA);\n        \n        v_status := \u0027SUCESSO\u0027;\n        v_mensagem := \u0027Senha alterada com sucesso pela primeira vez.\u0027;\n    EXCEPTION\n        WHEN OTHERS THEN\n            v_status := \u0027ERRO\u0027;\n            v_mensagem := \u0027Erro - \u0027 || SQLERRM;\n            \n            LOG_INSERT(:APP_USER, v_acao, v_status, v_mensagem);\n            \n            RAISE;\n    END;\n\n    LOG_INSERT(:APP_USER, v_acao, v_status, v_mensagem);\nEND;"
      }
