       ,"PL\/SQL Code":"DECLARE\n    v_status VARCHAR2(20);\n    v_mensagem VARCHAR(4000);\n    v_acao VARCHAR2(30);\nBEGIN\n    v_acao := \u0027Revogar role para role\u0027;\n    LOG_INSERT(:APP_USER, v_acao, \u0027INFO\u0027, (\u0027Tentativa de revoga\u00E7\u00E3o da role \u0027 || \u0027\"\u0027 || UPPER(:P11_ROLE_ATRIBUIDA) || \u0027\"\u0027 || \u0027 para a role \u0027 || \u0027\"\u0027 || UPPER(:P11_ROLE_BASE) || \u0027\"\u0027));\n    \n    BEGIN\n        pk_gis.revogar_role_role(REPLACE(:P11_ROLE_ATRIBUIDA, \u0027RL_GIS_\u0027, \u0027\u0027), REPLACE(:P11_ROLE_BASE, \u0027RL_GIS_\u0027, \u0027\u0027));\n\n        v_status := \u0027SUCESSO\u0027;\n        v_mensagem := \u0027Role revogada com sucesso.\u0027;\n    EXCEPTION\n        WHEN OTHERS THEN\n            v_status := \u0027ERRO\u0027;\n            v_mensagem := \u0027Erro - \u0027 || SQLERRM;\n            \n            LOG_INSERT(:APP_USER, v_acao, v_status, v_mensagem);\n            RAISE;\n    END;\n\n    LOG_INSERT(:APP_USER, v_acao, v_status, v_mensagem);\nEND;"
