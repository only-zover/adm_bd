      "id":"155211197792114610"
     ,"Identification":{
        "Name":"Teste de Estresse"
       ,"Type":"Executar C\u00F3digo"
       ,"Execution Chain":"None"
      }
     ,"Source":{
        "Location":"Local Database"
       ,"Language":"PL\/SQL"
       ,"PL\/SQL Code":"DECLARE\n    v_inicio_tempo NUMBER;\n    v_fim_tempo NUMBER;\n    v_tempo_total NUMBER;\n    v_numero_execucoes NUMBER := :P20_NUMERO_EXECUCOES;\n    v_segundos NUMBER;\n    v_centesimos NUMBER;\nBEGIN\n    v_inicio_tempo := DBMS_UTILITY.GET_TIME;\n\n    FOR i IN 1..:P20_NUMERO_EXECUCOES LOOP\n        TESTAR_ESTRESSE;\n    END LOOP;\n\n    v_fim_tempo := DBMS_UTILITY.GET_TIME;\n    v_tempo_total := (v_fim_tempo - v_inicio_tempo) \/ 100;\n\n    v_segundos := TRUNC(v_tempo_total);\n    v_centesimos := TRUNC(MOD(v_tempo_total * 100, 100));\n    \n    :P20_TEMPO_TOTAL := \u0027Tempo total de execu\u00E7\u00E3o: \u0027 || v_segundos || \u0027 segundos e \u0027 || v_centesimos || \u0027 cent\u00E9simos.\u0027;\nEND;"
      }
