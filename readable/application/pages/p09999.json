   ,{
      "id":"139402812223070910"
     ,"Identification":{
        "Name":"LOG"
       ,"Type":"Executar C\u00F3digo"
       ,"Execution Chain":"None"
      }
     ,"Source":{
        "Location":"Local Database"
       ,"Language":"PL\/SQL"
       ,"PL\/SQL Code":"DECLARE\n    v_status VARCHAR2(20);\n    v_mensagem VARCHAR2(4000);\n    v_user VARCHAR2(50);\n    v_acao VARCHAR2(20);\nBEGIN\n    v_acao := \u0027Login\u0027;\n    v_user := :APP_USER;\n    \n    IF v_user = \u0027nobody\u0027 THEN\n        v_user := \u0027Desconhecido\u0027;\n        v_mensagem := \u0027Erro: n\u00E3o foi poss\u00EDvel realizar o login.\u0027;\n        v_status := \u0027ERRO\u0027;\n    ELSE\n        v_status := \u0027SUCESSO\u0027;\n        v_mensagem := \u0027Login realizado com sucesso.\u0027;\n    END IF;\n\n    LOG_INSERT(\u0027Desconhecido\u0027, v_acao, \u0027INFO\u0027, (\u0027Tentativa de login com nome \u0027 || UPPER(:P9999_USERNAME)));\n    LOG_INSERT(v_user , v_acao, v_status, v_mensagem);\nEND;"
      }
     ,"Execution":{
        "Sequence":40
       ,"Point":"Processing"
       ,"Run Process":"Once Per Page Visit (default)"
      }
     ,"Error":{
        "Display Location":"Inline in Notification"
      }
    }
   ,{
      "id":"155210792334114606"
     ,"Identification":{
        "Name":"Inserir usu\u00E1rio na tabela de primeiro login"
       ,"Type":"Executar C\u00F3digo"
       ,"Execution Chain":"None"
      }
     ,"Source":{
        "Location":"Local Database"
       ,"Language":"PL\/SQL"
       ,"PL\/SQL Code":"DECLARE\n    v_count NUMBER;\n    v_username VARCHAR2(255);\nBEGIN\n    v_username := :APP_USER;\n\n    SELECT COUNT(*)\n    INTO v_count\n    FROM usuarios_primeiro_login\n    WHERE usuario = v_username;\n\n    IF v_count = 0 THEN\n        INSERT INTO usuarios_primeiro_login (usuario) VALUES (v_username);\n        COMMIT;\n    END IF;\nEND;"
      }
